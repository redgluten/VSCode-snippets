{
	"PHP Unit test": {
		"prefix": "test",
		"body": [
			"/** @test */",
			"function $1()",
			"{",
			"\t$0",
			"}"
		],
		"description": "Basic test"
	},
	"Method": {
		"prefix": "meth",
		"body": [
			"${1:public} function $2()",
			"{",
			"\t$3",
			"}"
		],
		"description": "Basic test"
	},
	"Laravel factory create": {
		"prefix": "facc",
		"body": [
			"\\$${1:machin} = factory(${2:Machin}::class)->create(${3:[$4]});",
		],
		"description": "Laravel factory create and store"
	},
	"Closure": {
		"prefix": "clos",
		"body": [
			"function (\\$${1:machin}) {",
            "\t${2:// do something}",
			"}$0",
		],
		"description": "PHP closure"
	},
	"Laravel Test": {
		"prefix": "laratest",
		"body": [
			"",
			"use Illuminate\\Foundation\\Testing\\WithoutMiddleware;",
			"use Illuminate\\Foundation\\Testing\\DatabaseMigrations;",
			"use Illuminate\\Foundation\\Testing\\DatabaseTransactions;",
			"",
			"class ${TM_FILENAME_BASE} extends TestCase",
			"{",
			"\t$0",
			"}",
		],
		"description": "Laravel test"
	},
}
